@model IEnumerable<Book_Shoppe.Entity.Book>
@{
    ViewBag.Title = "Geners";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="secondary-bg">
    <h1 class="heading">Geners</h1>
</div>

@if (Model.Count()==0)
{
