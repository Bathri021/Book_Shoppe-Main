@model IEnumerable<Book_Shoppe.Entity.Book>
@{
    ViewBag.Title = "Languages";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="secondary-bg">
    <h1 class="heading">Language</h1>
</div>

@if (Model.Count() == 0)
{
    <h3>Sorry There is no Books available in this genre</h3>
}
else
{
    <div>
        @Html.Partial("_EntityCardList")
    </div>
    <div>
        @Html.Partial("_EntityTableList")
    </div>
}